{
    "manifest_version": 2,
  
    "name": "Google-Meet-Themes",
    "description": "Color themes for Google Meets. Select a range of colors for Google Meet.",
    "version": "1.0.0",
    "icons": { "128": "images/brush.png" },
    "content_scripts": [
        {
            "matches": ["*://meet.google.com/*"],
            "js": ["js/script.js"]
        }
    ],
    

    "browser_action": {
      "default_popup": "html/popup.html"
    },

    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ]
  }